<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mytest"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="17"
        android:targetSdkVersion="19" />
    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>
    <application
        android:sharedUserId="android.uid.system"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.mytest.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.mytest.activities.ActivityForCameraTest"
            android:configChanges="screenSize|keyboardHidden|orientation"
            android:screenOrientation="portrait"
            android:label="@string/activity_camera" >
            <intent-filter>
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.mytest.activities.ActivityForTabHostTest"
            android:configChanges="screenSize|keyboardHidden|orientation"
            android:screenOrientation="portrait"
            android:label="@string/activity_tab_test">
            <intent-filter>
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.mytest.activities.ActivityForFragmentTest"
            android:configChanges="screenSize|keyboardHidden|orientation"
            android:screenOrientation="portrait"
            android:label="@string/activity_tab_test">
            <intent-filter>
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.mytest.activities.ActivityForImageAnimate"
            android:configChanges="screenSize|keyboardHidden|orientation"
            android:screenOrientation="portrait"
            android:label="@string/activity_image_animate">
            <intent-filter>
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.mytest.activities.ActivityForLedFlash"
            android:configChanges="screenSize|keyboardHidden|orientation"
            android:screenOrientation="portrait">
            <intent-filter>
            </intent-filter>
        </activity>
    </application>

</manifest>
